{
    "hooks": {
        "afterCreate": [
            "OutlookSync"
        ]
    },
    "fields": {
        "cWartung": {
            "type": "link"
        },
        "parent": {
            "entityList": [
                "Account",
                "Contact",
                "Lead",
                "Opportunity",
                "Case",
                "CWartung"
            ]
        },
        "collaborators": {
            "type": "linkMultiple",
            "view": "views/fields/collaborators",
            "maxCount": 30,
            "fieldManagerParamList": [
                "readOnly",
                "readOnlyAfterCreate",
                "audited",
                "autocompleteOnEmpty",
                "maxCount",
                "inlineEditDisabled",
                "tooltipText"
            ]
        },
        "cRepeatEnabled": {
            "notNull": true,
            "type": "bool",
            "isCustom": true
        },
        "cRepeatEvery": {
            "type": "int",
            "default": 1,
            "isCustom": true,
            "tooltip": true
        },
        "cRepeatUnit": {
            "type": "enum",
            "options": [
                "day",
                "week",
                "month"
            ],
            "style": {
                "day": null,
                "week": null,
                "month": null
            },
            "default": "day",
            "maxLength": 100,
            "isCustom": true
        },
        "cRepeatFrom": {
            "notNull": false,
            "type": "date",
            "isCustom": true
        },
        "cRepeatTo": {
            "notNull": false,
            "type": "date",
            "isCustom": true
        },
        "cRepeatSeriesId": {
            "type": "varchar",
            "maxLength": 100,
            "options": [],
            "isCustom": true
        }
    },
    "links": {
        "cWartung": {
            "type": "belongsTo",
            "foreign": "tasks",
            "entity": "CWartung",
            "audited": false,
            "isCustom": true
        },
        "collaborators": {
            "type": "hasMany",
            "entity": "User",
            "relationName": "entityCollaborator",
            "layoutRelationshipsDisabled": true
        }
    },
    "collection": {
        "orderBy": "createdAt",
        "order": "desc",
        "textFilterFields": [
            "name"
        ],
        "fullTextSearch": false,
        "countDisabled": false
    },
    "optimisticConcurrencyControl": true
}