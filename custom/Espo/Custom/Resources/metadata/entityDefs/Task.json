{
    "hooks": {
        "afterCreate": [
            "OutlookSync"
        ]
    },
    "fields": {
        "cWartung": {
            "type": "link"
        },
        "parent": {
            "entityList": [
                "Account",
                "Contact",
                "Lead",
                "Opportunity",
                "Case",
                "CWartung"
            ]
        },
        "collaborators": {
            "type": "linkMultiple",
            "view": "views/fields/collaborators",
            "maxCount": 30,
            "fieldManagerParamList": [
                "readOnly",
                "readOnlyAfterCreate",
                "audited",
                "autocompleteOnEmpty",
                "maxCount",
                "inlineEditDisabled",
                "tooltipText"
            ]
        }
    },
    "links": {
        "cWartung": {
            "type": "belongsTo",
            "foreign": "tasks",
            "entity": "CWartung",
            "audited": false,
            "isCustom": true
        },
        "collaborators": {
            "type": "hasMany",
            "entity": "User",
            "relationName": "entityCollaborator",
            "layoutRelationshipsDisabled": true
        }
    },
    "collection": {
        "orderBy": "createdAt",
        "order": "desc",
        "textFilterFields": [
            "name"
        ],
        "fullTextSearch": false,
        "countDisabled": false
    },
    "optimisticConcurrencyControl": true
}