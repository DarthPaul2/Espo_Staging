{
    "fields": {
        "name": {
            "type": "varchar",
            "required": true,
            "pattern": "$noBadCharacters"
        },
        "description": {
            "type": "text"
        },
        "createdAt": {
            "notNull": false,
            "type": "datetime",
            "readOnly": true,
            "minuteStep": 30,
            "tooltip": true
        },
        "modifiedAt": {
            "type": "datetime",
            "readOnly": true
        },
        "createdBy": {
            "type": "link",
            "readOnly": true,
            "view": "views/fields/user"
        },
        "modifiedBy": {
            "type": "link",
            "readOnly": true,
            "view": "views/fields/user"
        },
        "assignedUser": {
            "type": "link",
            "required": false,
            "view": "views/fields/assigned-user"
        },
        "teams": {
            "type": "linkMultiple",
            "view": "views/fields/teams"
        },
        "startzeit": {
            "notNull": false,
            "type": "datetime",
            "minuteStep": 1,
            "isCustom": true,
            "tooltip": true
        },
        "endzeit": {
            "notNull": false,
            "type": "datetime",
            "minuteStep": 1,
            "isCustom": true,
            "default": "javascript: return this.dateTime.getNow(1);",
            "tooltip": true
        },
        "startlat": {
            "notNull": false,
            "type": "float",
            "isCustom": true
        },
        "startlng": {
            "notNull": false,
            "type": "float",
            "isCustom": true
        },
        "endlat": {
            "notNull": false,
            "type": "float",
            "isCustom": true
        },
        "endlng": {
            "notNull": false,
            "type": "float",
            "isCustom": true
        },
        "pauseminuten": {
            "type": "int",
            "isCustom": true,
            "tooltip": true
        },
        "dauerminuten": {
            "type": "int",
            "isCustom": true,
            "tooltip": true
        },
        "nettominuten": {
            "type": "int",
            "isCustom": true,
            "tooltip": true
        },
        "autoended": {
            "notNull": true,
            "type": "bool",
            "isCustom": true
        },
        "status": {
            "type": "enum",
            "options": [
                "open",
                "closed",
                "corrected"
            ],
            "default": "open",
            "style": {
                "open": null,
                "closed": null,
                "corrected": null
            },
            "maxLength": 100,
            "isCustom": true
        },
        "ueberstundenminuten": {
            "type": "int",
            "isCustom": true,
            "tooltip": true
        },
        "feiertagwochenende": {
            "notNull": true,
            "type": "bool",
            "isCustom": true
        },
        "externalid": {
            "type": "varchar",
            "maxLength": 100,
            "options": [],
            "isCustom": true
        },
        "techniker": {
            "type": "link"
        },
        "startkarte": {
            "type": "url",
            "isCustom": true,
            "tooltip": true
        },
        "endkarte": {
            "type": "url",
            "isCustom": true,
            "tooltip": true
        }
    },
    "links": {
        "createdBy": {
            "type": "belongsTo",
            "entity": "User"
        },
        "modifiedBy": {
            "type": "belongsTo",
            "entity": "User"
        },
        "assignedUser": {
            "type": "belongsTo",
            "entity": "User"
        },
        "teams": {
            "type": "hasMany",
            "entity": "Team",
            "relationName": "entityTeam",
            "layoutRelationshipsDisabled": true
        },
        "techniker": {
            "type": "belongsTo",
            "foreign": "cArbeitszeiten",
            "entity": "User",
            "audited": false,
            "isCustom": true
        }
    },
    "collection": {
        "orderBy": "createdAt",
        "order": "desc"
    },
    "indexes": {
        "name": {
            "columns": [
                "name",
                "deleted"
            ]
        },
        "assignedUser": {
            "columns": [
                "assignedUserId",
                "deleted"
            ]
        },
        "createdAt": {
            "columns": [
                "createdAt"
            ]
        },
        "createdAtId": {
            "unique": true,
            "columns": [
                "createdAt",
                "id"
            ]
        }
    }
}