{
    "fields": {
        "name": {
            "type": "varchar",
            "required": true,
            "pattern": "$noBadCharacters"
        },
        "description": {
            "type": "text"
        },
        "createdAt": {
            "type": "datetime",
            "readOnly": true
        },
        "modifiedAt": {
            "type": "datetime",
            "readOnly": true
        },
        "createdBy": {
            "type": "link",
            "readOnly": true,
            "view": "views/fields/user"
        },
        "modifiedBy": {
            "type": "link",
            "readOnly": true,
            "view": "views/fields/user"
        },
        "assignedUser": {
            "type": "link",
            "required": false,
            "view": "views/fields/assigned-user"
        },
        "teams": {
            "type": "linkMultiple",
            "view": "views/fields/teams"
        },
        "cObjektnummer": {
            "type": "varchar",
            "maxLength": 100,
            "options": [],
            "isCustom": true
        },
        "cStrasseHausnum": {
            "type": "varchar",
            "maxLength": 100,
            "options": [],
            "isCustom": true
        },
        "cPLZ": {
            "type": "varchar",
            "maxLength": 20,
            "options": [],
            "isCustom": true
        },
        "cOrt": {
            "type": "varchar",
            "maxLength": 100,
            "options": [],
            "isCustom": true
        },
        "cLand": {
            "type": "varchar",
            "maxLength": 100,
            "default": "Deutschland",
            "options": [],
            "isCustom": true
        },
        "cBezeichnungZusatz": {
            "type": "varchar",
            "maxLength": 255,
            "options": [],
            "tooltip": true,
            "isCustom": true
        },
        "account": {
            "type": "link"
        },
        "contact": {
            "type": "link"
        }
    },
    "links": {
        "createdBy": {
            "type": "belongsTo",
            "entity": "User"
        },
        "modifiedBy": {
            "type": "belongsTo",
            "entity": "User"
        },
        "assignedUser": {
            "type": "belongsTo",
            "entity": "User"
        },
        "teams": {
            "type": "hasMany",
            "entity": "Team",
            "relationName": "entityTeam",
            "layoutRelationshipsDisabled": true
        },
        "account": {
            "type": "belongsTo",
            "foreign": "cObjekts",
            "entity": "Account",
            "audited": false,
            "isCustom": true
        },
        "angebots": {
            "type": "hasMany",
            "foreign": "objekt",
            "entity": "CAngebot",
            "audited": false,
            "isCustom": true
        },
        "rechnungs": {
            "type": "hasMany",
            "foreign": "objekt",
            "entity": "CRechnung",
            "audited": false,
            "isCustom": true
        },
        "contact": {
            "type": "belongsTo",
            "foreign": "cObjekts",
            "entity": "Contact",
            "audited": false,
            "isCustom": true
        },
        "wartungs": {
            "type": "hasMany",
            "foreign": "objekt",
            "entity": "CWartung",
            "audited": false,
            "isCustom": true
        }
    },
    "collection": {
        "orderBy": "createdAt",
        "order": "desc"
    },
    "indexes": {
        "name": {
            "columns": [
                "name",
                "deleted"
            ]
        },
        "assignedUser": {
            "columns": [
                "assignedUserId",
                "deleted"
            ]
        },
        "createdAt": {
            "columns": [
                "createdAt"
            ]
        },
        "createdAtId": {
            "unique": true,
            "columns": [
                "createdAt",
                "id"
            ]
        }
    }
}