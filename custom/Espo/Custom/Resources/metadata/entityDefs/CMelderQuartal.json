{
    "hooks": {
        "afterRemove": {
            "className": "Espo\\Custom\\Hooks\\CMelderQuartal\\SyncDelete"
        }
    },
    "fields": {
        "name": {
            "type": "varchar",
            "required": true,
            "pattern": "$noBadCharacters"
        },
        "description": {
            "type": "text"
        },
        "createdAt": {
            "type": "datetime",
            "readOnly": true
        },
        "modifiedAt": {
            "type": "datetime",
            "readOnly": true
        },
        "createdBy": {
            "type": "link",
            "readOnly": true,
            "view": "views/fields/user"
        },
        "modifiedBy": {
            "type": "link",
            "readOnly": true,
            "view": "views/fields/user"
        },
        "assignedUser": {
            "type": "link",
            "required": false,
            "view": "views/fields/assigned-user"
        },
        "teams": {
            "type": "linkMultiple",
            "view": "views/fields/teams"
        },
        "account": {
            "type": "link"
        },
        "user": {
            "type": "link"
        },
        "year": {
            "type": "int",
            "isCustom": true
        },
        "quarter": {
            "type": "enum",
            "options": [
                "Q1",
                "Q2",
                "Q3",
                "Q4"
            ],
            "style": {
                "Q1": null,
                "Q2": null,
                "Q3": null,
                "Q4": null
            },
            "default": "Q1",
            "maxLength": 100,
            "isCustom": true
        },
        "filledAt": {
            "notNull": false,
            "type": "datetime",
            "minuteStep": 30,
            "isCustom": true
        },
        "updatedAtServer": {
            "notNull": false,
            "type": "datetime",
            "minuteStep": 30,
            "isCustom": true
        },
        "technikerId": {
            "type": "varchar",
            "maxLength": 100,
            "options": [],
            "isCustom": true
        },
        "pdfPath": {
            "type": "varchar",
            "maxLength": 100,
            "options": [],
            "isCustom": true
        },
        "pdfUrl": {
            "type": "url",
            "isCustom": true
        },
        "sourceId": {
            "type": "varchar",
            "maxLength": 100,
            "options": [],
            "isCustom": true
        }
    },
    "links": {
        "createdBy": {
            "type": "belongsTo",
            "entity": "User"
        },
        "modifiedBy": {
            "type": "belongsTo",
            "entity": "User"
        },
        "assignedUser": {
            "type": "belongsTo",
            "entity": "User"
        },
        "teams": {
            "type": "hasMany",
            "entity": "Team",
            "relationName": "entityTeam",
            "layoutRelationshipsDisabled": true
        },
        "account": {
            "type": "belongsTo",
            "foreign": "cMelderQuartals",
            "entity": "Account",
            "audited": false,
            "isCustom": true
        },
        "user": {
            "type": "belongsTo",
            "foreign": "cMelderQuartals",
            "entity": "User",
            "audited": false,
            "isCustom": true
        }
    },
    "collection": {
        "orderBy": "createdAt",
        "order": "desc",
        "textFilterFields": [
            "name"
        ],
        "fullTextSearch": false,
        "countDisabled": false
    },
    "indexes": {
        "name": {
            "columns": [
                "name",
                "deleted"
            ]
        },
        "assignedUser": {
            "columns": [
                "assignedUserId",
                "deleted"
            ]
        },
        "createdAt": {
            "columns": [
                "createdAt"
            ]
        },
        "createdAtId": {
            "unique": true,
            "columns": [
                "createdAt",
                "id"
            ]
        },
        "sourceIdUnique": {
            "unique": true,
            "columns": [
                "sourceId",
                "deleted"
            ]
        }
    },
    "optimisticConcurrencyControl": false
}