{
    "hooks": {
        "beforeSave": [
            "AutoNumber",
            "FillLieferadresse",
            "CleanPositions"
        ],
        "afterSave": [
            "ImportFromAuftrag"
        ],
        "afterRelate": {
            "angebot": [
                "ImportFromAngebot"
            ]
        }
    },
    "fields": {
        "name": {
            "type": "varchar",
            "required": false,
            "pattern": "$noBadCharacters",
            "options": []
        },
        "description": {
            "type": "text"
        },
        "createdAt": {
            "type": "datetime",
            "readOnly": true
        },
        "modifiedAt": {
            "type": "datetime",
            "readOnly": true
        },
        "createdBy": {
            "type": "link",
            "readOnly": true,
            "view": "views/fields/user"
        },
        "modifiedBy": {
            "type": "link",
            "readOnly": true,
            "view": "views/fields/user"
        },
        "assignedUser": {
            "type": "link",
            "required": false,
            "view": "views/fields/assigned-user"
        },
        "teams": {
            "type": "linkMultiple",
            "view": "views/fields/teams"
        },
        "auftrag": {
            "type": "link",
            "inlineEditDisabled": false
        },
        "account": {
            "type": "link"
        },
        "accountKundenNr": {
            "readOnly": true,
            "type": "foreign",
            "link": "account",
            "field": "cKundennummer",
            "view": "views/fields/foreign-varchar",
            "isCustom": true
        },
        "serviceNummer": {
            "type": "varchar",
            "maxLength": 100,
            "options": [],
            "isCustom": true
        },
        "lieferdatum": {
            "notNull": false,
            "type": "date",
            "isCustom": true
        },
        "bemerkung": {
            "type": "text",
            "rowsMin": 2,
            "cutHeight": 200,
            "isCustom": true
        },
        "pdfUrl": {
            "type": "url",
            "isCustom": true
        },
        "betragNetto": {
            "notNull": false,
            "type": "float",
            "readOnlyAfterCreate": true,
            "isCustom": true
        },
        "betragBrutto": {
            "notNull": false,
            "type": "float",
            "readOnlyAfterCreate": true,
            "isCustom": true
        },
        "ustBetrag": {
            "notNull": false,
            "type": "float",
            "readOnlyAfterCreate": true,
            "isCustom": true
        },
        "einleitung": {
            "type": "text",
            "rowsMin": 2,
            "cutHeight": 200,
            "isCustom": true
        },
        "lieferscheinnummer": {
            "type": "varchar",
            "maxLength": 100,
            "options": [],
            "readOnlyAfterCreate": true,
            "isCustom": true
        },
        "angebot": {
            "type": "link"
        },
        "rechnung": {
            "type": "link"
        },
        "versendetAm": {
            "notNull": false,
            "type": "datetime",
            "minuteStep": 30,
            "isCustom": true
        },
        "kontaktperson": {
            "type": "link"
        },
        "lieferadresse": {
            "type": "address",
            "viewMap": false,
            "inlineEditDisabled": false,
            "isCustom": true
        }
    },
    "links": {
        "createdBy": {
            "type": "belongsTo",
            "entity": "User"
        },
        "modifiedBy": {
            "type": "belongsTo",
            "entity": "User"
        },
        "assignedUser": {
            "type": "belongsTo",
            "entity": "User"
        },
        "teams": {
            "type": "hasMany",
            "entity": "Team",
            "relationName": "entityTeam",
            "layoutRelationshipsDisabled": true
        },
        "auftrag": {
            "type": "belongsTo",
            "foreign": "lieferscheins",
            "entity": "CAuftrag",
            "audited": false,
            "isCustom": true
        },
        "account": {
            "type": "belongsTo",
            "foreign": "cLieferscheins",
            "entity": "Account",
            "audited": false,
            "isCustom": true
        },
        "lieferscheinpositions": {
            "type": "hasMany",
            "foreign": "lieferschein",
            "entity": "CLieferscheinposition",
            "audited": false,
            "isCustom": true
        },
        "angebot": {
            "type": "belongsTo",
            "foreign": "lieferscheins",
            "entity": "CAngebot",
            "audited": false,
            "isCustom": true
        },
        "rechnung": {
            "type": "belongsTo",
            "foreign": "lieferscheins",
            "entity": "CRechnung",
            "audited": false,
            "isCustom": true
        },
        "kontaktperson": {
            "type": "belongsTo",
            "foreign": "cLieferscheins",
            "entity": "Contact",
            "audited": false,
            "isCustom": true
        }
    },
    "collection": {
        "orderBy": "createdAt",
        "order": "desc"
    },
    "indexes": {
        "name": {
            "columns": [
                "name",
                "deleted"
            ]
        },
        "assignedUser": {
            "columns": [
                "assignedUserId",
                "deleted"
            ]
        },
        "createdAt": {
            "columns": [
                "createdAt"
            ]
        },
        "createdAtId": {
            "unique": true,
            "columns": [
                "createdAt",
                "id"
            ]
        }
    }
}