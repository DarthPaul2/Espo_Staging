            "type": "enum",
            "options": [
                "1",
                "2",
                "3",
                "4",
                "5"
            ],
            "style": {
                "1": null,
                "2": null,
                "3": null,
                "4": null,
                "5": null
            },
            "default": "1",
            "maxLength": 100,
            "isCustom": true
        },
        "datum": {
            "notNull": false,
