{
    "hooks": {
        "afterSave": [
            "TriggerServerRecalc"
        ],
        "afterRemove": [
            "TriggerServerRecalc"
        ]
    },
    "fields": {
        "name": {
            "type": "varchar",
            "required": false,
            "pattern": "$noBadCharacters",
            "options": []
        },
        "description": {
            "type": "text"
        },
        "createdAt": {
            "type": "datetime",
            "readOnly": true
        },
        "modifiedAt": {
            "type": "datetime",
            "readOnly": true
        },
        "createdBy": {
            "type": "link",
            "readOnly": true,
            "view": "views/fields/user"
        },
        "modifiedBy": {
            "type": "link",
            "readOnly": true,
            "view": "views/fields/user"
        },
        "assignedUser": {
            "type": "link",
            "required": false,
            "view": "views/fields/assigned-user"
        },
        "teams": {
            "type": "linkMultiple",
            "view": "views/fields/teams"
        },
        "auftrag": {
            "type": "link"
        },
        "angebot": {
            "type": "link"
        },
        "angebotsposition": {
            "type": "link"
        },
        "material": {
            "type": "link"
        },
        "materialPreis": {
            "type": "foreign",
            "link": "material",
            "field": "preis",
            "view": "views/fields/foreign-float",
            "readOnly": true,
            "isCustom": true
        },
        "materialEinheit": {
            "type": "foreign",
            "link": "material",
            "field": "einheit",
            "view": "views/fields/foreign-varchar",
            "readOnly": true,
            "isCustom": true
        },
        "beschreibung": {
            "type": "text",
            "rowsMin": 2,
            "cutHeight": 200,
            "isCustom": true
        },
        "einheit": {
            "type": "varchar",
            "maxLength": 100,
            "options": [],
            "isCustom": true
        },
        "menge": {
            "notNull": false,
            "type": "float",
            "isCustom": true
        },
        "preis": {
            "notNull": false,
            "type": "float",
            "isCustom": true
        },
        "netto": {
            "notNull": false,
            "type": "float",
            "isCustom": true,
            "dbField": "netto",
            "exportDisabled": false,
            "importDisabled": false,
            "readOnly": false
        },
        "gesamt": {
            "notNull": false,
            "type": "float",
            "isCustom": true,
            "dbField": "gesamt",
            "exportDisabled": false,
            "importDisabled": false,
            "readOnly": false
        },
        "includeInAuftrag": {
            "type": "bool",
            "isCustom": true,
            "dbField": "include_in_auftrag",
            "default": true,
            "notNull": true,
            "exportDisabled": false,
            "importDisabled": false
        },
        "rabatt": {
            "notNull": false,
            "type": "float",
            "isCustom": true
        },
        "steuer": {
            "type": "varchar",
            "maxLength": 100,
            "options": [],
            "isCustom": true
        },
        "einkaufspreis": {
            "notNull": false,
            "type": "float",
            "isCustom": true
        },
        "sortierung": {
            "type": "int",
            "isCustom": true
        },
        "angebotOption12": {
            "readOnly": true,
            "type": "foreign",
            "link": "angebot",
            "field": "gesetzOption12",
            "view": "views/fields/foreign-bool",
            "isCustom": true
        },
        "angebotOption13b": {
            "readOnly": true,
            "type": "foreign",
            "link": "angebot",
            "field": "gesetzOption13b",
            "view": "views/fields/foreign-bool",
            "isCustom": true
        },
        "bereitsabgerechnet": {
            "type": "bool",
            "default": false,
            "isCustom": true
        }
    },
    "links": {
        "createdBy": {
            "type": "belongsTo",
            "entity": "User"
        },
        "modifiedBy": {
            "type": "belongsTo",
            "entity": "User"
        },
        "assignedUser": {
            "type": "belongsTo",
            "entity": "User"
        },
        "teams": {
            "type": "hasMany",
            "entity": "Team",
            "relationName": "entityTeam",
            "layoutRelationshipsDisabled": true
        },
        "auftrag": {
            "type": "belongsTo",
            "foreign": "auftragspositions",
            "entity": "CAuftrag",
            "audited": false,
            "isCustom": true
        },
        "angebot": {
            "type": "belongsTo",
            "foreign": "auftragspositions",
            "entity": "CAngebot",
            "audited": false,
            "isCustom": true
        },
        "angebotsposition": {
            "type": "belongsTo",
            "foreign": "auftragspositions",
            "entity": "CAngebotsposition",
            "audited": false,
            "isCustom": true
        },
        "material": {
            "type": "belongsTo",
            "foreign": "auftragspositions",
            "entity": "CMaterial",
            "audited": false,
            "isCustom": true
        },
        "rechnungspositions": {
            "type": "hasMany",
            "foreign": "auftragsposition",
            "entity": "CRechnungsposition",
            "audited": false,
            "isCustom": true
        }
    },
    "collection": {
        "orderBy": "createdAt",
        "order": "desc"
    },
    "indexes": {
        "name": {
            "columns": [
                "name",
                "deleted"
            ]
        },
        "assignedUser": {
            "columns": [
                "assignedUserId",
                "deleted"
            ]
        },
        "createdAt": {
            "columns": [
                "createdAt"
            ]
        },
        "createdAtId": {
            "unique": true,
            "columns": [
                "createdAt",
                "id"
            ]
        }
    }
}