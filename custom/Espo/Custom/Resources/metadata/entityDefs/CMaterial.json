{
    "fields": {
        "name": {
            "type": "varchar",
            "required": true,
            "pattern": "$noBadCharacters"
        },
        "description": {
            "type": "text"
        },
        "createdAt": {
            "type": "datetime",
            "readOnly": true
        },
        "modifiedAt": {
            "type": "datetime",
            "readOnly": true
        },
        "createdBy": {
            "type": "link",
            "readOnly": true,
            "view": "views/fields/user"
        },
        "modifiedBy": {
            "type": "link",
            "readOnly": true,
            "view": "views/fields/user"
        },
        "assignedUser": {
            "type": "link",
            "required": false,
            "view": "views/fields/assigned-user"
        },
        "teams": {
            "type": "linkMultiple",
            "view": "views/fields/teams"
        },
        "appklesecId": {
            "type": "int",
            "isCustom": true
        },
        "code": {
            "type": "varchar",
            "maxLength": 50,
            "options": [],
            "isCustom": true
        },
        "barcode": {
            "type": "varchar",
            "maxLength": 100,
            "options": [],
            "isCustom": true
        },
        "kategorie": {
            "type": "varchar",
            "maxLength": 100,
            "options": [],
            "isCustom": true
        },
        "einheit": {
            "type": "varchar",
            "maxLength": 50,
            "options": [],
            "isCustom": true
        },
        "preis": {
            "notNull": false,
            "type": "float",
            "isCustom": true
        },
        "verwendet": {
            "notNull": true,
            "type": "bool",
            "isCustom": true
        },
        "aktiv": {
            "notNull": true,
            "type": "bool",
            "isCustom": true
        }
    },
    "links": {
        "createdBy": {
            "type": "belongsTo",
            "entity": "User"
        },
        "modifiedBy": {
            "type": "belongsTo",
            "entity": "User"
        },
        "assignedUser": {
            "type": "belongsTo",
            "entity": "User"
        },
        "teams": {
            "type": "hasMany",
            "entity": "Team",
            "relationName": "entityTeam",
            "layoutRelationshipsDisabled": true
        },
        "positionen": {
            "type": "hasMany",
            "foreign": "material",
            "entity": "CAngebotsposition",
            "audited": false,
            "isCustom": true
        },
        "rechnungspositions": {
            "type": "hasMany",
            "foreign": "material",
            "entity": "CRechnungsposition",
            "audited": false,
            "isCustom": true
        },
        "lieferscheinpositions": {
            "type": "hasMany",
            "foreign": "material",
            "entity": "CLieferscheinposition",
            "audited": false,
            "isCustom": true
        },
        "auftragspositions": {
            "type": "hasMany",
            "foreign": "material",
            "entity": "CAuftragsposition",
            "audited": false,
            "isCustom": true
        }
    },
    "collection": {
        "orderBy": "createdAt",
        "order": "desc"
    },
    "indexes": {
        "name": {
            "columns": [
                "name",
                "deleted"
            ]
        },
        "assignedUser": {
            "columns": [
                "assignedUserId",
                "deleted"
            ]
        },
        "createdAt": {
            "columns": [
                "createdAt"
            ]
        },
        "createdAtId": {
            "unique": true,
            "columns": [
                "createdAt",
                "id"
            ]
        }
    }
}