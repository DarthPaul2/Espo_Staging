{
    "fields": {
        "name": {
            "type": "varchar",
            "required": false,
            "pattern": "$noBadCharacters",
            "options": []
        },
        "description": {
            "type": "text"
        },
        "createdAt": {
            "type": "datetime",
            "readOnly": true
        },
        "modifiedAt": {
            "type": "datetime",
            "readOnly": true
        },
        "createdBy": {
            "type": "link",
            "readOnly": true,
            "view": "views/fields/user"
        },
        "modifiedBy": {
            "type": "link",
            "readOnly": true,
            "view": "views/fields/user"
        },
        "assignedUser": {
            "type": "link",
            "required": false,
            "view": "views/fields/assigned-user"
        },
        "teams": {
            "type": "linkMultiple",
            "view": "views/fields/teams"
        },
        "anlageTyp": {
            "type": "enum",
            "required": true,
            "options": [
                "bma",
                "ema",
                "video",
                "zutritt",
                "other"
            ],
            "style": {
                "bma": null,
                "ema": null,
                "video": null,
                "zutritt": null,
                "other": null
            },
            "default": "bma",
            "maxLength": 100,
            "isCustom": true
        },
        "intervall": {
            "type": "enum",
            "required": true,
            "options": [
                "monatlich",
                "quartal",
                "halbjaehrlich",
                "jaehrlich"
            ],
            "style": {
                "monatlich": null,
                "quartal": null,
                "halbjaehrlich": null,
                "jaehrlich": null
            },
            "default": "quartal",
            "maxLength": 100,
            "isCustom": true
        },
        "startDatum": {
            "notNull": false,
            "type": "date",
            "required": true,
            "isCustom": true
        },
        "regelModus": {
            "type": "enum",
            "required": true,
            "options": [
                "abLetzterWartung",
                "abStartdatum"
            ],
            "style": {
                "abLetzterWartung": null,
                "abStartdatum": null
            },
            "default": "abLetzterWartung",
            "maxLength": 100,
            "isCustom": true
        },
        "letzteWartung": {
            "notNull": false,
            "type": "date",
            "isCustom": true
        },
        "naechsteWartung": {
            "notNull": false,
            "type": "date",
            "isCustom": true
        },
        "vorwarnTage": {
            "type": "int",
            "isCustom": true
        },
        "faelligkeitsStatus": {
            "type": "enum",
            "options": [
                "faellig",
                "baldFaellig",
                "nichtFaellig",
                "beendet"
            ],
            "style": {
                "faellig": "danger",
                "baldFaellig": "warning",
                "nichtFaellig": "success",
                "beendet": "info"
            },
            "default": "faellig",
            "maxLength": 100,
            "isCustom": true
        },
        "status": {
            "type": "enum",
            "options": [
                "aktiv",
                "ausgesetzt",
                "beendet"
            ],
            "style": {
                "aktiv": null,
                "ausgesetzt": null,
                "beendet": null
            },
            "default": "aktiv",
            "maxLength": 100,
            "isCustom": true
        },
        "planSlot": {
            "type": "enum",
            "options": [
                "none",
                "q1",
                "q2",
                "q3",
                "q4"
            ],
            "style": {
                "none": null,
                "q1": null,
                "q2": null,
                "q3": null,
                "q4": null
            },
            "default": "none",
            "maxLength": 100,
            "isCustom": true
        },
        "faelligkeitsTag": {
            "type": "int",
            "isCustom": true
        },
        "autoAufgabeErstellen": {
            "notNull": true,
            "type": "bool",
            "tooltip": true,
            "isCustom": true
        },
        "autoBenachrichtigen": {
            "notNull": true,
            "type": "bool",
            "isCustom": true
        },
        "prioritaet": {
            "type": "enum",
            "options": [
                "hoch",
                "normal",
                "niedrig"
            ],
            "style": {
                "hoch": "warning",
                "normal": "success",
                "niedrig": "info"
            },
            "default": "normal",
            "maxLength": 100,
            "isCustom": true
        },
        "bemerkung": {
            "type": "text",
            "rowsMin": 2,
            "cutHeight": 200,
            "isCustom": true
        },
        "externalWartungId": {
            "type": "varchar",
            "maxLength": 100,
            "options": [],
            "tooltip": true,
            "isCustom": true
        },
        "vertragsNummer": {
            "type": "varchar",
            "maxLength": 100,
            "options": [],
            "isCustom": true
        }
    },
    "links": {
        "createdBy": {
            "type": "belongsTo",
            "entity": "User"
        },
        "modifiedBy": {
            "type": "belongsTo",
            "entity": "User"
        },
        "assignedUser": {
            "type": "belongsTo",
            "entity": "User"
        },
        "teams": {
            "type": "hasMany",
            "entity": "Team",
            "relationName": "entityTeam",
            "layoutRelationshipsDisabled": true
        }
    },
    "collection": {
        "orderBy": "createdAt",
        "order": "desc"
    },
    "indexes": {
        "name": {
            "columns": [
                "name",
                "deleted"
            ]
        },
        "assignedUser": {
            "columns": [
                "assignedUserId",
                "deleted"
            ]
        },
        "createdAt": {
            "columns": [
                "createdAt"
            ]
        },
        "createdAtId": {
            "unique": true,
            "columns": [
                "createdAt",
                "id"
            ]
        }
    }
}